{
    "src/main/java/*.java": {"type": "src", "alternate": "src/test/java/{}Test.java"},
    "src/test/java/*Test.java": {"type": "test", "alternate": "src/main/java/{}.java"},
    "pom.xml": {"type": "pom"},
    "*": {"make": "mvn"},
    "src/main/resources/application.properties": {"type": "aprop"},
    "src/main/resources/*": {"type": "res"}
}
